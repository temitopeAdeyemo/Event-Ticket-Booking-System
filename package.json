{
  "name": "event-ticket-booking-system",
  "version": "1.0.0",
  "description": "EventModel Ticket Booking System",
  "main": "src/server.js",
  "scripts": {
    "start": "npm run test && 'ts-node' -r newrelic dist/server.js",
    "compile": "tsc",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' -r newrelic dist/server.js",
    "test": "NODE_ENV=test jest ./dist/tests --coverage --detectOpenHandles",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/config/DataBase.config.ts ./src/shared/migrations/migrations",
    "migration:up": "typeorm-ts-node-commonjs migration:run -d ./src/config/DataBase.config.ts",
    "migration:down": "typeorm-ts-node-commonjs migration:revert -d ./src/config/DataBase.config.ts"
  },
  "author": "Adeyemo Temitope Julius",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "ioredis": "^5.4.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.47",
    "morgan": "^1.10.0",
    "newrelic": "^12.11.3",
    "node-cron": "^3.0.3",
    "path": "^0.12.7",
    "pg": "^8.13.1",
    "rate-limiter-flexible": "^5.0.3",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.3",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^3.0.1",
    "prettier": "2.8.8",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5"
  }
}
